#include <stdlib.h> 

int main()

{

    int num;

    int originalnum; 

    int sumofdigits = 0;

    scanf("%d", &num);

    originalnum = num; 

    if (abs(originalnum) < 100) 

    {

        printf("Invalid Input\n");

        return 0; 

    }

    num = abs(num);

    while (num > 0) 

    {

        sumofdigits += num % 10; 

        num /= 10;                 

    }

    printf("Sum of digit is %d\n", sumofdigits);

    return 0;

}
