#include <stdio.h>

int main() {

    char str[1000], result[1000];

    int i = 0, j = 0;

    // Read input string (including spaces)

    fgets(str, sizeof(str), stdin);

    // Traverse and copy only non-space characters

    while (str[i] != '\0') {

        if (str[i] != ' ' && str[i] != '\n') {

            result[j++] = str[i];

        }

        i++;

    }

    // Null-terminate the result string

    result[j] = '\0';

    // Print the result

    printf("%s\n", result);

    return 0;

}
